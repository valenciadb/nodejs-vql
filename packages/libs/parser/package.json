{
  "name": "@vql/parser",
  "version": "0.0.0",
  "description": "ValenciaQL query parser.",
  "homepage": "https://valenciadb.com",
  "preferGlobal": false,
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Sia Codelabs",
    "email": "hello@siacodelabs.com",
    "url": "https://siacodelabs.com"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/valenciadb/nodejs-vql/issues",
    "email": "bugs@valenciadb.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valenciadb/nodejs-vql.git"
  },
  "dependencies": {
    "antlr4": "^4.11.0"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.3"
  },
  "scripts": {
    "build": "npm run build/antlr && npm run build/src && npm run build/src/post",
    "build/antlr": "rm -rf src/antlr && npm run build/antlr/lexer && npm run build/antlr/parser",
    "build/antlr/lexer": "cd grammar && antlr4 -o ../src/antlr Lexer.g4",
    "build/antlr/parser": "cd grammar && antlr4 -no-listener -o ../src/antlr Vql.g4",
    "build/src": "rm -rf ./dist/ && tsc --project tsconfig.json",
    "build/src/post": "sed -i 's/antlr4\\/error\\/index.js/antlr4\\/src\\/antlr4\\/error\\/index.js/' dist/esm/Parser.js",
    "lint": "eslint --fix",
    "test": "mocha --config .mocharc.yaml",
    "cov": "c8 mocha --config .mocharc.yaml"
  }
}
